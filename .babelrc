{
  "presets": [
    ["es2015"],
    "react",
    "stage-0"
  ],
  "plugins": [
    ["react-hot-loader/babel"],
    ["transform-object-rest-spread"],
    [
      "transform-runtime", {
        "polyfill": false,
      }
    ],
    [
      "module-resolver", {
        "root": ["./src"]
      }
    ]

  ]
}
