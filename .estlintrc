{
  "extends": "standard",
  "env": {
    "es6": true,
    "browser": true,
    "jest": true,
    "node": true,
  },
  "plugins": [
    "react"
  ],
  "globals": {
    module: true
  },
  "ecmaFeatures": {
    "modules": true
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    }
  }
}
